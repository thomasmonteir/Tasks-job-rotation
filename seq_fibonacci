#include <iostream>

using namespace std;

int main() {
    int num;
    int first = 0, second = 1, next;

    cout << "Digite um número: ";
    cin >> num;

    while (next < num) {
        next = first + second;
        first = second;
        second = next;
    }

    if (next == num) {
        cout << num << " Pertence à sequência de Fibonacci." << endl;
    } else {
        cout << num << " Nao pertence à sequência de Fibonacci." << endl;
    }

    return 0;
}
