#include <iostream>

using namespace std;

int main() {
   
    float sp = 67836.43;
    float rj = 36678.66;
    float mg = 29229.88;
    float es = 27165.48;
    float outros = 19849.53;
  
    float total = sp + rj + mg + es + outros;
    
    // Cálculo do percentual de representação de cada estado
    float perc_sp = (sp / total) * 100;
    float perc_rj = (rj / total) * 100;
    float perc_mg = (mg / total) * 100;
    float perc_es = (es / total) * 100;
    float perc_outros = (outros / total) * 100;

    // Impressão dos resultados
    cout << "Percentual de representacao de cada estado no valor total mensal da distribuidora:\n" << endl;
    cout << "SP: " << perc_sp << "%" << endl;
    cout << "RJ: " << perc_rj << "%" << endl;
    cout << "MG: " << perc_mg << "%" << endl;
    cout << "ES: " << perc_es << "%" << endl;
    cout << "Outros: " << perc_outros << "%" << endl;
    
    return 0;
}
